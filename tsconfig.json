{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"useUnknownInCatchVariables": false,
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@index/*": ["src/pages/index/*"],
			"@img/*": ["src/assets/images/*"],
			"@layouts/*": ["src/layouts/*"],
			"@main/*": ["src/pages/main/*"],
			"@svg/*": ["src/assets/svg/*"],
			"@assets/*": ["src/assets/*"]
		}
	}
}
